                        <p class="item-name"> <%= item.produto %>
                        <span id="summary-quantity"> <%= item.qtde %></span> x R$ <%= item.preco.toFixed(2) %></span>
                        </p>
                    </article>
            <%
                });
            } else {
            %>
            <%
            }
            %>
        </article>
