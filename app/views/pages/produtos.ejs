                function mostrarNotificacao() {
                    return new Notify ({
                        status: '<%= dadosNotificacao.type %>',
                        title: '<%= dadosNotificacao.msg %>', 
                        text: '' + campoInvalido.replace(/&lt;/g,"<").replace(/&gt;/g,">"),
                        speed: 300,
                        effect: 'fade',
                        showIcon: false,
                        showCloseButton: true,
                        autoclose: true,
                        autotimeout: 5000,
                        gap: 20,
                        distance: 20,
                        position: 'center'
                    });
                }

