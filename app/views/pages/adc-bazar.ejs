    <% 
        let msgErro = ''
            
        const campoErro = {   
            Nome:'',
            Ano:'',
            Descricao: '',
            Titulo: '',
            Biografia: '',
            imgBazar: ''

        };
        if (listaErros) {
            listaErros.errors.forEach(err => {
                campoErro[err['path']] = 'invalido';
                msgErro +=  err.msg + '<br>'
                %>     
            <%});%>
            <script>
                (() => {
                    var campoInvalido = '<%= msgErro%>'
                    function mostrarNotificacao() {
                        return new Notify ({
                            status: 'error',
                            title: 'Verifique os Campos', 
                            text: '' + campoInvalido.replace(/&lt;/g,"<").replace(/&gt;/g,">"),
                            speed: 300,
                            effect: 'fade',
                            showIcon: false,
                            showCloseButton: true,
                            autoclose: true,
                            autotimeout: 5000,
                            gap: 20,
                            distance: 20,
                            position: 'center'
                        });
                    }

                    mostrarNotificacao();
                })();
            </script>
        <% } %>
